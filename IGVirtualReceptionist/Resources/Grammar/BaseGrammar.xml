<grammar version="1.0" xml:lang="en-US" root="rootRule" tag-format="semantics/1.0-literals" xmlns="http://www.w3.org/2001/06/grammar">
  <rule id="rootRule">
    <one-of>
      <item>
        <tag>BACK</tag>
        <one-of>
          <item>back</item>
          <item>go back</item>
        </one-of>
      </item>
      <item>
        <tag>HOME</tag>
        <one-of>
          <item>home</item>
          <item>go home</item>
        </one-of>
      </item>
      <item>
        <tag>HELLO</tag>
        <one-of>
          <item>hello</item>
          <item>hi</item>
        </one-of>
      </item>
      <item>
        <tag>HELP</tag>
        <one-of>
          <item>help</item>
          <item>help me</item>
        </one-of>
      </item>
    </one-of>
  </rule>
</grammar>